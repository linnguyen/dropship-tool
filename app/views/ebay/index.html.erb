                '</GetSellerListRequest>';

            $.ajax({
                type: "POST",
                url: wsUrl,
                contentType: "text/xml",
                dataType: "xml",
                data: soapRequest1,
                success: processSuccess,
                error: processError,
                contentType: "text/xml; charset=\"utf-8\"",

