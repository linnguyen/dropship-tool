            var soapRequest1 = '<?xml version="1.0" encoding="utf-8"?>\n' +
                '<GetSellerListRequest xmlns="urn:ebay:apis:eBLBaseComponents">\n' +
                '  <RequesterCredentials>\n' +
                '    <eBayAuthToken>AgAAAA**AQAAAA**aAAAAA**YDCHXA**nY+sHZ2PrBmdj6wVnY+sEZ2PrA2dj6ADloOoCZCHoQydj6x9nY+seQ**8toFAA**AAMAAA**</eBayAuthToken>\n' +
